{
	"name": "typo3/cms-core",
	"type": "typo3-cms-framework",
	"description": "The core library of TYPO3.",
	"homepage": "https://typo3.org",
	"license": ["GPL-2.0-or-later"],
	"authors": [{
		"name": "TYPO3 Core Team",
		"email": "typo3cms@typo3.org",
		"role": "Developer"
	}],
	"support": {
		"chat": "https://typo3.org/help",
		"docs": "https://docs.typo3.org",
		"issues": "https://forge.typo3.org",
		"source": "https://github.com/TYPO3/TYPO3.CMS"
	},
	"config": {
		"sort-packages": true
	},
	"require": {
		"php": "^7.2",
		"ext-PDO": "*",
		"ext-json": "*",
		"ext-pcre": "*",
		"ext-session": "*",
		"ext-xml": "*",
		"cogpowered/finediff": "0.3.1",
		"doctrine/annotations": "1.10.2",
		"doctrine/dbal": "2.10.2",
		"doctrine/instantiator": "1.3.0",
		"doctrine/lexer": "1.2.0",
		"egulias/email-validator": "2.1.17",
		"guzzlehttp/guzzle": "6.5.3",
		"nikic/php-parser": "4.4.0",
		"psr/container": "1.0.0",
		"psr/event-dispatcher": "1.0.0",
		"psr/http-client": "1.0.0",
		"psr/http-factory": "1.0.1",
		"psr/http-message": "1.0.1",
		"psr/http-server-handler": "1.0.1",
		"psr/http-server-middleware": "1.0.1",
		"psr/log": "1.1.3",
		"symfony/config": "5.0.7",
		"symfony/console": "5.0.7",
		"symfony/dependency-injection": "5.0.7",
		"symfony/expression-language": "5.0.7",
		"symfony/finder": "5.0.7",
		"symfony/http-foundation": "5.0.7",
		"symfony/mailer": "5.0.7",
		"symfony/mime": "5.0.7",
		"symfony/polyfill-intl-icu": "1.15.0",
		"symfony/polyfill-intl-idn": "1.15.0",
		"symfony/polyfill-mbstring": "1.15.0",
		"symfony/routing": "5.0.7",
		"symfony/yaml": "5.0.7",
		"typo3/class-alias-loader": "1.0.2",
		"typo3/cms-cli": "2.0.0",
		"typo3/cms-composer-installers": "2.2.4",
		"typo3/phar-stream-wrapper": "3.1.4",
		"typo3fluid/fluid": "2.6.9"
	},
	"require-dev": {
		"codeception/codeception": "4.1.4",
		"codeception/module-asserts": "1.2.1",
		"codeception/module-filesystem": "1.0.2",
		"codeception/module-webdriver": "1.0.7",
		"friendsofphp/php-cs-fixer": "2.16.3",
		"phpspec/prophecy": "1.10.3",
		"phpstan/phpstan": "0.12.19",
		"typo3/cms-styleguide": "10.0.3",
		"typo3/testing-framework": "6.2.3"
	},
	"suggest": {
		"ext-fileinfo": "Used for proper file type detection in the file abstraction layer",
		"ext-gd": "GDlib/Freetype is required for building images with text (GIFBUILDER) and can also be used to scale images",
		"ext-intl": "TYPO3 with unicode-based filesystems",
		"ext-mysqli": "",
		"ext-openssl": "OpenSSL is required for sending SMTP mails over an encrypted channel endpoint, and for extensions such as \"rsaauth\"",
		"ext-zip": "",
		"ext-zlib": "TYPO3 uses zlib for amongst others output compression and un/packing t3x extension files"
	},
	"conflict": {
		"hoa/core": "*",
		"guzzlehttp/guzzle": "6.5.0",
		"typo3/cms": "*"
	},
	"replace": {
		"typo3/cms-lang": "*",
		"typo3/cms-saltedpasswords": "*",
		"typo3/cms-sv": "*"
	},
	"provide": {
		"psr/http-client-implementation": "1.0",
		"psr/http-factory-implementation": "1.0",
		"psr/http-message-implementation": "1.0"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "10.4.x-dev"
		},
		"typo3/cms": {
			"Package": {
				"serviceProvider": "TYPO3\\CMS\\Core\\ServiceProvider",
				"protected": true,
				"partOfFactoryDefault": true,
				"partOfMinimalUsableSystem": true
			},
			"extension-key": "core"
		},
		"typo3/class-alias-loader": {
			"class-alias-maps": [
				"Migrations/Code/ClassAliasMap.php"
			]
		}
	},
	"autoload": {
		"psr-4": {
			"TYPO3\\CMS\\Core\\": "Classes/"
		},
		"classmap": ["Resources/PHP/"],
		"files": ["Resources/PHP/GlobalDebugFunctions.php"]
	},
	"autoload-dev": {
		"psr-4": {
			"TYPO3\\CMS\\Core\\Tests\\": "Tests/"
		},
		"classmap": [
			"Tests/Unit/Core/Fixtures/test_extension/",
			"Tests/Functional/Fixtures/"
		]
	}
}
